# This is Git's per-user configuration file.
[user]
# Please adapt and uncomment the following lines:
	name = Chris Wynne
	email = christopher.wynne@digital.cabinet-office.gov.uk
[alias]
  co = checkout
  ci = commit
  st = status
  br = branch
  hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
  type = cat-file -t
  dump = cat-file -p
[push]
	default = simple
[core]
	editor = /usr/bin/vim
[fetch]
    # Remove deleted branches on fetch
    prune = true
[color]
    # Color command output when appropriate
    ui = auto
